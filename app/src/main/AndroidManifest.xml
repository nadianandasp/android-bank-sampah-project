<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.sahitya.banksampahsahitya">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".VerificationCodeActivity"></activity>
        <activity android:name=".RegisterSuccesfullyActivity" />
        <activity android:name=".RegisterActivity" />
        <activity android:name=".ForgotPasswordActivity" />
        <activity android:name=".presentation.membership.SettingsActivity" />
        <activity android:name=".presentation.membership.changepassword.ChangePasswordActivity" />
        <activity android:name=".presentation.membership.editprofile.EditProfileActivity" />
        <activity android:name=".presentation.membership.disbursement.DisbursementActivity" />
        <activity android:name=".presentation.membership.login.LoginActivity">
        </activity>
        <activity android:name=".SplashActivity" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity" />
    </application>

</manifest>